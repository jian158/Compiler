class fruit
{
    int kind;
    int taste;
    int value(int m, int n)
    {
        int result;
        kind = 1;
        if (m > 0)
        {
            if (m == 1){
                result = n * taste + kind;
            }
            else{
                result = n * taste;
            }
        }
        else{
            result = n * taste - kind;
        }
        return result;
    }
}

class Main
{
    void main()
    {
        int x;
        int y;
        fruit apple;
        apple = new fruit();
        apple.kind = 2;
        apple.taste = 6;
        apple.value(x, y);
    }
}
